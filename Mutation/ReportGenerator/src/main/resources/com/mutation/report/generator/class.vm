<html>
<head>

<STYLE TYPE='text/css'>

/* CSS generated at csscreator.com */
BODY{
	background-color:#FFFFFF;  
	color:#003399;  
	font-family:sans-serif;  
	font-size:small; 
	}
	
th { 
	color:#003399;  
	font-weight:bold;
	font-family:sans-serif;  
	font-size:small; 
  }

td { 
	color:grey; 
	font-family:monospace; 
	font-size:medium;  
}
.mut_survived {
  background-color:#FFAAAA;
  color:red; 
  font-weight:bold;
}
.mut_killed {
  background-color:#AAFFAA;
  color:green; 
  font-weight:bold;
}

</STYLE>
    
</head>
<body>
<table>
<th>No</th>
<th>Source Code</th>
<th>Name</th>
<th>Transition</th>
<th>Killer Tests</th>
#foreach( $sourceLine in $sourceInfo )

	#set( $line = $sourceLine.Text.replaceAll(" ","&#160;") )
	#set( $line = $line.replaceAll("\t","&#160;&#160;") )
	#if( $sourceLine.MutantInfo )

		#foreach( $mutant in $sourceLine.MutantInfo )
		<tr>
			<td>$sourceLine.LineNo</td>
		
			#if ( $mutant.isSurvived() )
				<td class="mut_survived">$line</td>
				<td class="mut_survived">$mutant.Name</td>
				<td class="mut_survived">$mutant.Transition</td>
				<td>&#160;</td>
			#else
				<td class="mut_killed">$line</td>
				<td class="mut_killed">$mutant.Name</td>
				<td class="mut_killed">$mutant.Transition</td>
				<td class="mut_killed">
				#foreach( $test in $mutant.KillerTests )
					$test<br/>
				#end
				</td>
				
			#end
		<tr>
		#end
	#else
		<tr>
			<td>$sourceLine.LineNo</td>
			<td>$line</td>
			<td>&#160;</td>
			<td>&#160;</td>
			<td>&#160;</td>
		</tr>
	#end
#end

</table>
</ul>
</body>
</html>