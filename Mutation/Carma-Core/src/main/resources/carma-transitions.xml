<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN" "http://www.springframework.org/dtd/spring-beans-2.0.dtd">

<beans>

	<!--  set of operators considered for tests -->

	<bean id="operators"
		class="com.retroduction.carma.core.transform.TransitionGroupConfig">
		<property name="transitionGroups">
			<list>
				<bean
					class="com.retroduction.carma.core.transform.asm.ror.ROR_TransitionGroup">
					<constructor-arg>
						<value>true</value>
					</constructor-arg>
				</bean>
			</list>
		</property>
	</bean>

	
</beans>